<h1>Inquiries</h1>

<table>
  <thead>
    <tr>
      <th>Inquiry Type</th>
      <th>Received</th>
      <th>Name</th>
      <th>City</th>
      <th>Event Date</th>
    </tr>
  </thead>
  <tbody>
    <% @inquiries.each do |inquiry| %>
    <tr>
      <td><%= inquiry.role %></td>
      <td><%= inquiry.created_at %></td>
      <td><%= inquiry.name %></td>
      <td><%= inquiry.city %></td>
      <td><%= inquiry.date %></td>
    </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Inquiry', new_inquiry_path %>